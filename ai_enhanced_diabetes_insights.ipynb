    "class AIConsultationSystem:\n",
    "    \"\"\"\n",
    "    Interactive AI-powered medical consultation system.\n",
    "    \n",
    "    Combines ML predictions with AI insights to provide \n",
    "    comprehensive patient consultations.\n",
    "    \"\"\"\n",
    "    \n",
    "    def __init__(self, model_artifacts, ai_assistant):\n",
    "        self.model = model_artifacts['model']\n",
    "        self.feature_columns = model_artifacts['feature_columns']\n",
    "        self.feature_importance = model_artifacts['feature_importance']\n",
    "        self.ai_assistant = ai_assistant\n",
    "        self.consultation_history = []\n",
    "    \n",
    "    def conduct_consultation(self, patient_data, consultation_id=None):\n",
    "        \"\"\"\n",
    "        Conduct a comprehensive AI-enhanced consultation.\n",
    "        \n",
    "        Args:\n",
    "            patient_data (dict): Patient medical measurements\n",
    "            consultation_id (str): Unique consultation identifier\n",
    "        \n",
    "        Returns:\n",
    "            dict: Complete consultation results\n",
    "        \"\"\"\n",
    "        if not consultation_id:\n",
    "            consultation_id = f\"CONSULT_{datetime.now().strftime('%Y%m%d_%H%M%S')}\"\n",
    "        \n",
    "        # Step 1: ML Risk Assessment\n",
    "        input_df = pd.DataFrame([patient_data])[self.feature_columns]\n",
    "        prediction = self.model.predict(input_df)[0]\n",
    "        probability = self.model.predict_proba(input_df)[0, 1]\n",
    "        \n",
    "        # Risk stratification\n",
    "        if probability < 0.2:\n",
    "            risk_category = \"Very Low Risk\"\n",
    "            urgency = \"Routine\"\n",
    "        elif probability < 0.4:\n",
    "            risk_category = \"Low Risk\"\n",
    "            urgency = \"Routine\"\n",
    "        elif probability < 0.6:\n",
    "            risk_category = \"Moderate Risk\"\n",
    "            urgency = \"Follow-up Recommended\"\n",
    "        elif probability < 0.8:\n",
    "            risk_category = \"High Risk\"\n",
    "            urgency = \"Medical Consultation Advised\"\n",
    "        else:\n",
    "            risk_category = \"Very High Risk\"\n",
    "            urgency = \"Immediate Medical Attention\"\n",
    "        \n",
    "        ml_results = {\n",
    "            'prediction': int(prediction),\n",
    "            'probability': float(probability),\n",
    "            'risk_category': risk_category,\n",
    "            'urgency': urgency\n",
    "        }\n",
    "        \n",
    "        # Step 2: AI Enhancement\n",
    "        ai_analysis = self.ai_assistant.analyze_patient_risk(\n",
    "            patient_data, ml_results, self.feature_importance\n",
    "        )\n",
    "        \n",
    "        # Step 3: Generate Consultation Report\n",
    "        consultation_report = {\n",
    "            'consultation_id': consultation_id,\n",
    "            'timestamp': datetime.now().isoformat(),\n",
    "            'patient_data': patient_data,\n",
    "            'ml_assessment': ml_results,\n",
    "            'ai_insights': ai_analysis,\n",
    "            'consultation_summary': self._generate_consultation_summary(\n",
    "                patient_data, ml_results, ai_analysis\n",
    "            )\n",
    "        }\n",
    "        \n",
    "        # Store in history\n",
    "        self.consultation_history.append(consultation_report)\n",
    "        \n",
    "        return consultation_report\n",
    "    \n",
    "    def _generate_consultation_summary(self, patient_data, ml_results, ai_analysis):\n",
    "        \"\"\"\n",
    "        Generate a structured consultation summary.\n",
    "        \"\"\"\n",
    "        return {\n",
    "            'primary_assessment': f\"Diabetes risk assessment: {ml_results['risk_category']} ({ml_results['probability']*100:.1f}% probability)\",\n",
    "            'key_findings': self._extract_key_findings(patient_data),\n",
    "            'action_items': self._extract_action_items(ai_analysis['recommendations']),\n",
    "            'follow_up': ml_results['urgency'],\n",
    "            'patient_education': ai_analysis['patient_communication']\n",
    "        }\n",
    "    \n",
    "    def _extract_key_findings(self, patient_data):\n",
    "        \"\"\"Extract key clinical findings from patient data.\"\"\"\n",
    "        findings = []\n",
    "        \n",
    "        if patient_data.get('Glucose', 0) > 126:\n",
    "            findings.append(f\"🩸 Elevated glucose: {patient_data['Glucose']} mg/dL (normal <100)\")\n",
    "        \n",
    "        if patient_data.get('BMI', 0) > 30:\n",
    "            findings.append(f\"⚖️ Obesity: BMI {patient_data['BMI']} (normal 18.5-24.9)\")\n",
    "        elif patient_data.get('BMI', 0) > 25:\n",
    "            findings.append(f\"⚖️ Overweight: BMI {patient_data['BMI']} (normal 18.5-24.9)\")\n",
    "        \n",
    "        if patient_data.get('Age', 0) > 45:\n",
    "            findings.append(f\"📅 Age-related risk: {patient_data['Age']} years\")\n",
    "        \n",
    "        if patient_data.get('DiabetesPedigreeFunction', 0) > 0.5:\n",
    "            findings.append(f\"🧬 Family history factor: {patient_data['DiabetesPedigreeFunction']:.3f}\")\n",
    "        \n",
    "        return findings\n",
    "    \n",
    "    def _extract_action_items(self, recommendations):\n",
    "        \"\"\"Extract actionable items from AI recommendations.\"\"\"\n",
    "        # Simple parsing of recommendations\n",
    "        action_items = []\n",
    "        for line in recommendations.split('\\n'):\n",
    "            if line.strip():\n",
    "                action_items.append(line.strip())\n",
    "        return action_items[:5]  # Top 5 actions\n",
    "    \n",
    "    def display_consultation(self, consultation_report):\n",
    "        \"\"\"Display a formatted consultation report.\"\"\"\n",
    "        report = consultation_report\n",
    "        \n",
    "        print(f\"\\n{'='*100}\")\n",
    "        print(f\"🏥 AI-ENHANCED MEDICAL CONSULTATION REPORT\")\n",
    "        print(f\"{'='*100}\")\n",
    "        print(f\"📋 Consultation ID: {report['consultation_id']}\")\n",
    "        print(f\"⏰ Timestamp: {report['timestamp']}\")\n",
    "        \n",
    "        # Primary Assessment\n",
    "        print(f\"\\n🎯 PRIMARY ASSESSMENT:\")\n",
    "        print(f\"   {report['consultation_summary']['primary_assessment']}\")\n",
    "        print(f\"   Urgency Level: {report['ml_assessment']['urgency']}\")\n",
    "        \n",
    "        # Key Findings\n",
    "        print(f\"\\n🔍 KEY CLINICAL FINDINGS:\")\n",
    "        for finding in report['consultation_summary']['key_findings']:\n",
    "            print(f\"   • {finding}\")\n",
    "        \n",
    "        # Action Items\n",
    "        print(f\"\\n✅ RECOMMENDED ACTIONS:\")\n",
    "        for i, action in enumerate(report['consultation_summary']['action_items'], 1):\n",
    "            print(f\"   {i}. {action}\")\n",
    "        \n",
    "        # Patient Education\n",
    "        print(f\"\\n💬 PATIENT COMMUNICATION:\")\n",
    "        print(f\"   {report['consultation_summary']['patient_education']}\")\n",
    "        \n",
    "        # Follow-up\n",
    "        print(f\"\\n📅 FOLLOW-UP PLAN:\")\n",
    "        print(f\"   {report['consultation_summary']['follow_up']}\")\n",
    "        \n",
    "        print(f\"\\n{'='*100}\")\n",
    "\n",
    "# Initialize consultation system\n",
    "consultation_system = AIConsultationSystem(model_artifacts, ai_assistant)\n",
    "\n",
    "print(\"🏥 AI Consultation System initialized!\")\n",
    "print(\"Ready to conduct comprehensive patient consultations.\")"
