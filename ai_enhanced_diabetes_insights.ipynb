    "            recommendations.append(\"🩸 Monitor blood glucose levels regularly\")\n",
    "            recommendations.append(\"🍎 Follow a low-glycemic diet plan\")\n",
    "        \n",
    "        # BMI-based recommendations\n",
    "        bmi = patient_data.get('BMI', 0)\n",
    "        if bmi > 25:\n",
    "            recommendations.append(\"🏃‍♀️ Implement regular physical activity (150 min/week)\")\n",
    "            recommendations.append(\"⚖️ Consider weight management program\")\n",
    "        \n",
    "        # Age-based recommendations\n",
    "        age = patient_data.get('Age', 0)\n",
    "        if age > 45:\n",
    "            recommendations.append(\"📅 Schedule regular diabetes screening (annually)\")\n",
    "        \n",
    "        # General recommendations based on risk level\n",
    "        if probability > 0.7:\n",
    "            recommendations.append(\"🏥 Consult endocrinologist for comprehensive evaluation\")\n",
    "            recommendations.append(\"💊 Discuss preventive medication options\")\n",
    "        elif probability > 0.3:\n",
    "            recommendations.append(\"👨‍⚕️ Regular follow-up with primary care physician\")\n",
    "            recommendations.append(\"📊 Consider continuous glucose monitoring\")\n",
    "        else:\n",
    "            recommendations.append(\"✅ Maintain current healthy lifestyle\")\n",
    "            recommendations.append(\"🔄 Continue routine health screenings\")\n",
    "        \n",
    "        return \"\\n\".join(recommendations)\n",
    "    \n",
    "    def generate_population_insights(self, dataset_stats, model_performance):\n",
    "        \"\"\"Generate AI insights about the entire patient population.\"\"\"\n",
    "        if self.gemini_available:\n",
    "            return self._generate_ai_population_insights(dataset_stats, model_performance)\n",
    "        else:\n",
    "            return self._generate_mock_population_insights(dataset_stats, model_performance)\n",
    "    \n",
    "    def _generate_mock_population_insights(self, stats, performance):\n",
    "        \"\"\"Generate mock population insights.\"\"\"\n",
    "        return {\n",
    "            \"population_analysis\": f\"\"\"Analysis of {stats['total_patients']} patients reveals a {stats['diabetes_rate']*100:.1f}% \n",
    "            diabetes prevalence. The population shows diverse risk profiles with key patterns in glucose regulation, \n",
    "            weight management, and age-related risk factors.\"\"\",\n",
    "            \n",
    "            \"risk_patterns\": \"\"\"Three distinct risk patterns emerge: 1) Young, healthy individuals with minimal risk factors, \n",
    "            2) Middle-aged patients with metabolic syndrome indicators, and 3) Older patients with multiple comorbidities. \n",
    "            Each group requires tailored prevention strategies.\"\"\",\n",
    "            \n",
    "            \"clinical_recommendations\": f\"\"\"Healthcare system recommendations: Implement tiered screening protocols, \n",
    "            focus prevention programs on high-risk demographics, and leverage AI-assisted diagnosis to improve \n",
    "            early detection rates. Current model accuracy of {performance['accuracy']*100:.1f}% supports clinical deployment.\"\"\",\n",
    "            \n",
    "            \"business_impact\": \"\"\"Potential healthcare cost savings through early intervention, reduced emergency \n",
    "            department visits, and improved patient outcomes. AI-enhanced screening can process 10x more patients \n",
    "            with consistent quality assessment.\"\"\"\n",
    "        }\n",
    "\n",
    "# Initialize AI system\n",
    "GEMINI_API_KEY = os.getenv('GEMINI_API_KEY', None)  # Set your API key as environment variable\n",
    "ai_assistant = AIMedicalIntelligence(api_key=GEMINI_API_KEY)\n",
    "\n",
    "print(\"🧠 AI Medical Intelligence System ready!\")"
   ]
