    "class AIMedicalIntelligence:\n",
    "    \"\"\"\n",
    "    AI-Enhanced Medical Analysis System\n",
    "    \n",
    "    Combines traditional ML predictions with generative AI insights\n",
    "    to provide human-like medical reasoning and explanations.\n",
    "    \"\"\"\n",
    "    \n",
    "    def __init__(self, api_key=None):\n",
    "        \"\"\"Initialize the AI Medical Intelligence system.\"\"\"\n",
    "        self.gemini_available = False\n",
    "        self.model = None\n",
    "        \n",
    "        # Configure Gemini if available\n",
    "        if GEMINI_AVAILABLE and api_key:\n",
    "            try:\n",
    "                genai.configure(api_key=api_key)\n",
    "                self.model = genai.GenerativeModel('gemini-pro')\n",
    "                self.gemini_available = True\n",
    "                print(\"🤖 Gemini AI Medical Assistant initialized\")\n",
    "            except Exception as e:\n",
    "                print(f\"⚠️ Gemini initialization failed: {e}\")\n",
    "                print(\"📝 Using mock responses for demonstration\")\n",
    "        else:\n",
    "            print(\"📝 Using mock AI responses (set GEMINI_API_KEY for real AI)\")\n",
    "    \n",
    "    def analyze_patient_risk(self, patient_data, prediction_result, feature_importance):\n",
    "        \"\"\"\n",
    "        Generate AI-powered analysis of patient diabetes risk.\n",
    "        \n",
    "        Args:\n",
    "            patient_data (dict): Patient medical measurements\n",
    "            prediction_result (dict): ML model prediction results\n",
    "            feature_importance (dict): Feature importance from ML model\n",
    "        \n",
    "        Returns:\n",
    "            dict: Comprehensive AI analysis\n",
    "        \"\"\"\n",
    "        if self.gemini_available:\n",
    "            return self._generate_ai_analysis(patient_data, prediction_result, feature_importance)\n",
    "        else:\n",
    "            return self._generate_mock_analysis(patient_data, prediction_result, feature_importance)\n",
    "    \n",
    "    def _generate_ai_analysis(self, patient_data, prediction_result, feature_importance):\n",
    "        \"\"\"Generate real AI analysis using Gemini.\"\"\"\n",
    "        prompt = f\"\"\"\n",
    "        As a medical AI assistant specializing in diabetes risk assessment, analyze this patient case:\n",
    "        \n",
    "        PATIENT DATA:\n",
    "        {json.dumps(patient_data, indent=2)}\n",
    "        \n",
    "        ML MODEL PREDICTION:\n",
    "        - Diabetes Risk: {'High' if prediction_result['prediction'] == 1 else 'Low'}\n",
    "        - Probability: {prediction_result['probability']:.3f} ({prediction_result['probability']*100:.1f}%)\n",
    "        - Risk Level: {prediction_result.get('risk_level', 'Unknown')}\n",
    "        \n",
    "        FEATURE IMPORTANCE (Top factors):\n",
    "        {json.dumps(dict(list(feature_importance.items())[:5]), indent=2)}\n",
    "        \n",
    "        Please provide:\n",
    "        1. RISK ASSESSMENT: Explain the prediction in medical terms\n",
    "        2. KEY FACTORS: Analyze the most important risk factors for this patient\n",
    "        3. CLINICAL INSIGHTS: What medical patterns do you observe?\n",
    "        4. RECOMMENDATIONS: Specific prevention/management strategies\n",
    "        5. PATIENT COMMUNICATION: How to explain this to the patient\n",
    "        \n",
    "        Format as JSON with these keys: risk_assessment, key_factors, clinical_insights, recommendations, patient_communication\n",
    "        \"\"\"\n",
    "        \n",
    "        try:\n",
    "            response = self.model.generate_content(prompt)\n",
    "            # Parse JSON response\n",
    "            analysis_text = response.text\n",
    "            # Extract JSON from response (simple parsing)\n",
    "            start_idx = analysis_text.find('{')\n",
    "            end_idx = analysis_text.rfind('}') + 1\n",
    "            if start_idx != -1 and end_idx != -1:\n",
    "                json_str = analysis_text[start_idx:end_idx]\n",
    "                return json.loads(json_str)\n",
    "            else:\n",
    "                # Fallback to structured text parsing\n",
    "                return self._parse_ai_response(analysis_text)\n",
    "        except Exception as e:\n",
    "            print(f\"AI analysis error: {e}\")\n",
    "            return self._generate_mock_analysis(patient_data, prediction_result, feature_importance)\n",
    "    \n",
    "    def _generate_mock_analysis(self, patient_data, prediction_result, feature_importance):\n",
    "        \"\"\"Generate mock AI analysis for demonstration.\"\"\"\n",
    "        risk_level = prediction_result.get('risk_level', 'Unknown')\n",
