    "def analyze_patient_with_ai(patient_data, patient_name=\"Patient\"):\n",
    "    \"\"\"\n",
    "    Perform comprehensive AI-enhanced analysis of a patient.\n",
    "    \n",
    "    Args:\n",
    "        patient_data (dict): Patient medical measurements\n",
    "        patient_name (str): Patient identifier for reporting\n",
    "    \n",
    "    Returns:\n",
    "        dict: Complete analysis with AI insights\n",
    "    \"\"\"\n",
    "    print(f\"\\n{'='*80}\")\n",
    "    print(f\"🏥 AI-ENHANCED ANALYSIS: {patient_name.upper()}\")\n",
    "    print(f\"{'='*80}\")\n",
    "    \n",
    "    # 1. Traditional ML Prediction\n",
    "    input_df = pd.DataFrame([patient_data])[feature_columns]\n",
    "    prediction = model.predict(input_df)[0]\n",
    "    probability = model.predict_proba(input_df)[0, 1]\n",
    "    \n",
    "    # Determine risk level\n",
    "    if probability < 0.3:\n",
    "        risk_level = \"Low Risk\"\n",
    "    elif probability < 0.7:\n",
    "        risk_level = \"Moderate Risk\"\n",
    "    else:\n",
    "        risk_level = \"High Risk\"\n",
    "    \n",
    "    prediction_result = {\n",
    "        'prediction': int(prediction),\n",
    "        'probability': float(probability),\n",
    "        'risk_level': risk_level\n",
    "    }\n",
    "    \n",
    "    print(f\"\\n🤖 MACHINE LEARNING PREDICTION:\")\n",
    "    print(f\"   Diabetes Risk: {'POSITIVE' if prediction == 1 else 'NEGATIVE'}\")\n",
    "    print(f\"   Probability: {probability:.3f} ({probability*100:.1f}%)\")\n",
    "    print(f\"   Risk Level: {risk_level}\")\n",
    "    \n",
    "    # 2. AI-Enhanced Analysis\n",
    "    print(f\"\\n🧠 GENERATING AI INSIGHTS...\")\n",
    "    ai_analysis = ai_assistant.analyze_patient_risk(\n",
    "        patient_data, prediction_result, feature_importance_sorted\n",
    "    )\n",
    "    \n",
    "    # 3. Display AI Analysis\n",
    "    print(f\"\\n📋 RISK ASSESSMENT:\")\n",
    "    print(f\"   {ai_analysis['risk_assessment']}\")\n",
    "    \n",
    "    print(f\"\\n🔍 KEY RISK FACTORS:\")\n",
    "    print(f\"   {ai_analysis['key_factors']}\")\n",
    "    \n",
    "    print(f\"\\n🏥 CLINICAL INSIGHTS:\")\n",
    "    print(f\"   {ai_analysis['clinical_insights']}\")\n",
    "    \n",
    "    print(f\"\\n💡 RECOMMENDATIONS:\")\n",
    "    for line in ai_analysis['recommendations'].split('\\n'):\n",
    "        if line.strip():\n",
    "            print(f\"   {line}\")\n",
    "    \n",
    "    print(f\"\\n💬 PATIENT COMMUNICATION:\")\n",
    "    print(f\"   {ai_analysis['patient_communication']}\")\n",
    "    \n",
    "    return {\n",
    "        'patient_data': patient_data,\n",
    "        'ml_prediction': prediction_result,\n",
    "        'ai_analysis': ai_analysis,\n",
    "        'timestamp': datetime.now().isoformat()\n",
    "    }\n",
    "\n",
    "# Test with sample patients\n",
    "sample_patients = [\n",
    "    {\n",
    "        'name': 'High Risk Patient (Sarah, 50)',\n",
    "        'data': {\n",
